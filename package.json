    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-redux": "^8.1.3",
    "react-scripts": "5.0.1",
    "web-vitals": "^3.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@parcel/packager-raw-url": "2.10.2",
    "@parcel/transformer-sass": "2.10.2",
    "@parcel/transformer-webmanifest": "2.10.2",
